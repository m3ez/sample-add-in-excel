<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp">
  <Id>de1b1b3e-2b2a-4a1f-9c01-aaa000111222</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Your Name</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>

  <DisplayName DefaultValue="Mini Hello Excel"/>
  <Description DefaultValue="Smallest Excel Online add-in (task pane)"/>

  <!-- Change to your HTTPS origin -->
  <IconUrl DefaultValue="https://YOUR-DOMAIN/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://YOUR-DOMAIN/icon-80.png"/>
  <SupportUrl DefaultValue="https://YOUR-DOMAIN/"/>

  <AppDomains>
    <AppDomain>https://YOUR-DOMAIN</AppDomain>
  </AppDomains>

  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="ExcelApi"/>
    </Sets>
  </Requirements>

  <DefaultSettings>
    <SourceLocation DefaultValue="https://YOUR-DOMAIN/taskpane.html"/>
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>
</OfficeApp>
